    "formik": "^1.5.7",
    "fs-extra": "^8.0.1",
    "knex": "^0.19.5",
    "moment": "^2.24.0",
    "nedb": "^1.8.0",
    "node-static": "^0.7.11",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-confirm-alert": "^2.4.1",
    "react-dev-utils": "^9.0.1",
    "react-dom": "^16.8.6",
    "react-highlighter": "^0.4.3",
